{
  "name": "strength-tracker",
  "version": "1.0.0",
  "description": "Plan and track your strength training",
  "main": "index.js",
  "repository": "https://github.com/fjzink/strength-tracker.git",
  "author": "Joey Zink",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon",
